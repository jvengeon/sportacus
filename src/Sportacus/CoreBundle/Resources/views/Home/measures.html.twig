<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Date</th>
      {% for typeMeasure in  typeMeasures %}
          <th>{{ typeMeasure.name|capitalize }} ({{ typeMeasure.unit }})</th>
      {% endfor %}
    </tr>
  </thead>
  <tbody>
  {% for date, measure in measures %}
     <tr>
      <td>{{ date }}</td>
      {% for  measureEntity in measure %}
	      <td>{{ measureEntity.value }}</td>
      {% endfor %}
    </tr>
  {% endfor %}
  </tbody>
</table> 